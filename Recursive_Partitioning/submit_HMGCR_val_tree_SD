for p in '5e-6' '1e-5' '3e-5' '5e-5' '7e-5' '1e-4'
do
	for n in '1:500' '501:1000' '1001:1500' '1501:2000'
	do
		sbatch -J "P"$p"_"$n --array=10,25,50,100,200,300,400,500 ./submit_val_tree_SD $p $n 'Pheno'
		sbatch -J "N"$p"_"$n --array=10,25,50,100,200,300,400,500 ./submit_val_tree_SD $p $n 'None'
	done
done
